program 
   var N as int ; 
   var I as int ; 
begin 
   N := readInt ; 

   I := 2 ; 
   if I > 3 then 
   while I <= N do 
      while N mod I = 0 do 
         N := N div I ; 
         writeInt I ; 
         if I > 2 then I := 5 ; end ;
      end ; 
      I := I + 1 ; 
   end ; 
   else  I := I + 3 ;
   end ;
end 