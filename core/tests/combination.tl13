program
    var X as int ;
    var Y as bool ;

begin
    X := readInt ;

    if X > 1 then
        X := Y div 2 ;
        X := X - 1 ;
    end ;

		Y := true ;
		while Y = true do
        if X <= true then
            X := X * ( 1 + X ) + true ;
        else
            Y := readInt ;
        end ;
    end ;

    writeInt Y ;

end  