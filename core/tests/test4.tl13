program
    var X as int ;
    var Y as bool ;

begin
    X := readInt ;
    Y := readInt ;            
    if X > true then
        X := X div 2 ;
        X := X - 1 ;
    end ;

                Y := true ;
                while Y do
        if X <= 100 then
            X := X * ( 1 + Y ) ;
        else
            Y := false ;
        end ;
    end ;

    writeInt Y ;

end